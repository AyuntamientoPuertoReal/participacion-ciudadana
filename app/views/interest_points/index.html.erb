<p id="notice"><%= notice %></p>

<h1>Interest Points</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interest_points.each do |interest_point| %>
      <tr>
        <td><%= link_to 'Show', interest_point %></td>
        <td><%= link_to 'Edit', edit_interest_point_path(interest_point) %></td>
        <td><%= link_to 'Destroy', interest_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interest Point', new_interest_point_path %>
