<p id="notice"><%= notice %></p>

<h1>Incidence Types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incidence_types.each do |incidence_type| %>
      <tr>
        <td><%= link_to 'Show', incidence_type %></td>
        <td><%= link_to 'Edit', edit_incidence_type_path(incidence_type) %></td>
        <td><%= link_to 'Destroy', incidence_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Incidence Type', new_incidence_type_path %>
